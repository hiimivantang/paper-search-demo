# Required environment variables for Vercel deployment

# OpenAI API Key for embeddings
OPENAI_API_KEY=your_openai_api_key_here

# Milvus/Zilliz Cloud credentials
MILVUS_URI=https://your-cluster.vectordb.zillizcloud.com:19541
MILVUS_TOKEN=your_milvus_token_here

# Semantic Highlighter Model Deployment ID (optional)
SEMANTIC_HIGHLIGHTER_MODEL_ID=your_model_deployment_id
